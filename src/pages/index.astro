---
import '../styles/fonts.css';
import '../styles/index.css';
import '../styles/lastfm.css';
import '@material/web/typography/md-typescale-styles.css'
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />

		<!-- discord meta tags  -->
		<meta content="mias website" property="og:title" />
		<meta content="so silly! so silly! so silly! :3" property="og:description" />
		<meta content="https://mimi.23347.top" property="og:url" />
		<meta content="#cba6f7" data-react-helmet="true" name="theme-color" />
		<!-- twitter meta tags  -->
		<meta name="twitter:card" content="summary" />
		<meta name="twitter:site" content="@miaaaa2a" />
		<meta name="twitter:creator" content="@miaaaa2a" />

		<title>mimi tavern</title>

		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300&display=swap" rel="stylesheet">
		<script is:inline src="https://code.jquery.com/jquery-3.7.0.min.js"></script>
		<script>
			import '@material/web/all.js';
		</script>
		<link href="https://www.nerdfonts.com/assets/css/webfont.css" rel="stylesheet">
	</head>
	<body class="dark-mode">
		<div class="scrolling-bg">
			<span class="marquee">mimimimimimimimimimimimi</span>
			<span class="marquee">mimimimimimimimimimimimi</span>
		</div>
		<div class="content-box">
			<md-tabs id="tabs" class="tabs">
				<md-primary-tab>About me</md-primary-tab>
				<md-primary-tab>last.fm</md-primary-tab>
				<md-primary-tab>Credits</md-primary-tab>
			</md-tabs>
			<div id="content"></div>
		</div>
		<div class="social"></div>
		<script src="../scripts/index.js"></script>
		<script src="../scripts/theming.js"></script>
	</body>
</html>
